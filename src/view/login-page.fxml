<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="190.0" prefWidth="215.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxControllers.LoginPage">
   <children>
      <TextField fx:id="loginField" layoutX="25.0" layoutY="26.0" promptText="login" />
      <PasswordField fx:id="passwordField" layoutX="25.0" layoutY="70.0" promptText="password" />
      <Button layoutX="25.0" layoutY="121.0" mnemonicParsing="false" onAction="#validate" prefHeight="26.0" prefWidth="73.0" text="Log in" />
      <Button layoutX="112.0" layoutY="121.0" mnemonicParsing="false" onAction="#register" prefHeight="26.0" prefWidth="73.0" text="Register" />
   </children>
</AnchorPane>
